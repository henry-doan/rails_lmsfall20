<h1><%= @course.name %> Enrollments</h1>
<%= link_to 'add Enrollment', new_course_enrollment_path(@course)%>

<h2>Teachers</h2>
<ul>
  <% @teachers.each do |t| %>
    <li>
      <%= t.user.full_name %>
      <br />
      <%= link_to 'remove', [@course, t], method: :delete %>
    </li>
  <% end %>
</ul>
<h2>Tas</h2>
<ul>
  <% @tas.each do |ta| %>
    <li>
      <%= ta.user.full_name %>
      <br />
      <%= link_to 'remove', [@course, ta], method: :delete %>
    </li>
  <% end %>
</ul>
<h2>Students</h2>
<ul>
  <% @students.each do |s| %>
    <li>
      <%= s.user.full_name %>
      <br />
      <%= link_to 'remove', [@course, s], method: :delete %>
    </li>
  <% end %>
</ul>
